{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "inertiajs/inertia-laravel": "dev-master",
        "kalnoy/nestedset": "v6.x-dev",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.6",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "silber/bouncer": "dev-master",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-translatable": "^5.2",
        "spatie/pdf-to-text": "^1.51",
        "tightenco/ziggy": "^1.4",
        "tpetry/laravel-postgresql-enhanced": "^0.10.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-ide-helper": "^2.12",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.18",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.13",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "files": [
            "app/Application/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/Application/",
            "Domain\\": "app/Domain/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --write-mixin"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/area17/laravel-nestedset"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-ray",
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
